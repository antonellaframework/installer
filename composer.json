	"version": "1.0",
    "description": "Antonella Framework Official Installer.",
    "keywords": ["framework", "wordpress", "plugins", "installer"],
    "prefer-stable": true,
    "minimum-stability": "dev",
	"license": "MIT",
    "type": "project",
	"authors": [
        {
            "name": "David Turnes",
            "email": "d3turnes@gmail.com",
			"homepage": "https://github.com/d3turnes",
            "role": "Developer"
        }
